node( label: 'slave01' ) {
    stage('Preparation') {
        git 'https://github.com/changwonyoon/demo1.git';
    } 
    stage('Build') {
		sh "rm -rf /home/jenkins/workspace/demo1/demo1.o"
		sh "rm -rf /home/jenkins/workspace/demo1/demo1_out"
        sh "chown -R jenkins:jenkins /home/jenkins/workspace"        
        sh 'make'
    }  
}